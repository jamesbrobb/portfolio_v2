
{
  "routes": [{
      "path": "/",
      "redirectTo": "about"
    },
    {
      "path": "about",
      "githubLink": "/",
      "detailsURI": "routeComponents/home/README.md"
    }, {
      "path": "components",
      "children": [{
        "path": "common",
        "children": [
          {
            "path": "overlay",
            "children": [
              {
                "path": "color",
                "githubLink": "components/common/overlay/color",
                "docURI": "/lib-components/common/overlay/color/README.md",
                "controls": [{
                  "controlType": "select",
                  "key": "color",
                  "label": "Color",
                  "value": "blue",
                  "options": [
                    {"key": "blue", "value": "Blue"},
                    {"key": "pink", "value": "Pink"},
                    {"key": "green", "value": "Green"},
                    {"key": "grey", "value": "Grey"},
                    {"key": "red", "value": "Red"}
                  ]
                }, {
                  "controlType": "input",
                  "key": "allow-transition",
                  "label": "Allow Transition",
                  "type": "checkbox",
                  "value": "true"
                }],
                "examples": [
                  "color-overlay-example"
                ]
              }
            ]
          }
        ]
      }, {
        "path": "layout",
        "children": [
          {"path": "grid"},
          {"path": "flex-grid"},
          {"path": "responsive-container"},
          {"path": "page-header"}
        ]
      }, {
        "path": "media",
        "children": [
          {
            "path": "image",
            "children": [
              {"path": "fallback"},
              {"path": "image-component"}
            ]
          }
        ]
      }, {
        "path": "product",
        "children": [
          {
            "path": "lesson-plan",
            "children": [
              {"path": "card"},
              {"path": "hero"},
              {"path": "grid"},
              {"path": "header"},
              {"path": "vocabulary"}
            ]
          }, {
            "path": "my-library",
            "children": [
              {"path": "lesson-plan-card"}
            ]
          }
        ]
      }]
    }, {
      "path": "core",
      "children": [
        {"path": "analytics"},
        {"path": "commands"},
        {"path": "http"}
      ]
    }
  ],
  "ga-analytics": {
    "trackerId": "G-EJZC3TV0QZ",
    "trackerName": "",
    "cookieDomain": ""
  },
  "analytics" : {
    "page": {
      "trackType": "page",
      "properties": {
        "page_title": "{%title%}",
        "page_path": "{%path%}",
        "page_location": "{%href%}"
      }
    },
    "github-link" : {
      "type": "github-link",
      "trackType": "event",
      "properties": {
        "event_category": "links",
        "event_label": "{%link%}"
      }
    },
    "child-section": {
      "type": "child-section.expand",
      "trackType": "event",
      "properties": {
        "event_category": "page",
        "event_label": "{%label%}",
        "value": "{%value%}"
      }
    },
    "href-link": {
      "type": "href-link",
      "trackType": "event",
      "properties": {
        "event_category": "links",
        "event_label": "{%link%}"
      }
    },
    "menu": {
      "parent-item": {
        "type": "parent-item-click",
        "trackType": "event",
        "properties": {
          "event_category": "menu",
          "event_label": "{%label%}",
          "value": "{%value%}"
        }
      },
      "item": {
        "type": "item-click",
        "trackType": "event",
        "properties": {
          "event_category": "menu",
          "event_label": "{%label%}"
        }
      }
    }
  },
  "github-config": {
      "root": "https://github.com/jamesbrobb/portfolio/",
      "app": "tree/main/src/app/"
  }
}
