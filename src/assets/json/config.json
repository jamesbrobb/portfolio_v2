
{
  "routes": [{
      "path": "/",
      "redirectTo": "about"
    },
    {
      "path": "about",
      "page": {
        "githubLink": "/",
        "docURI": "details/home/README.md"
      }
    }, {
      "path": "components",
      "children": [{
        "path": "dynamic-loader"
      },{
        "path": "layout",
        "children": [
          {
            "path": "grid",
            "page": {
              "githubLink": "components/layout/grid",
              "docURI": "/libs/components/layout/grid/README.md",
              "controls": [{
                "controlType": "input",
                "key": "itemCount",
                "label": "Item count",
                "type": "number",
                "value": 10,
                "min": 1,
                "max": 20
              }],
              "examples": [
                "grid-layout-example"
              ]
            }
          },
          {
            "path": "flex-grid",
            "page": {
              "githubLink": "styles/flex.scss",
              "docURI": "details/flex-grid/README.md",
              "controls": [{
                "controlType": "input",
                "key": "itemCount",
                "label": "Item count",
                "type": "number",
                "value": 10,
                "min": 1,
                "max": 20
              }],
              "examples": [
                "flex-grid-example"
              ]
            }
          },
          {
            "path": "responsive-container",
            "page": {
              "githubLink": "components/responsive",
              "docURI": "/libs/components/responsive/container/README.md",
              "examples": [
                "responsive-container-example"
              ]
            }
          }
        ]
      }, {
        "path": "product",
        "children": [{
          "path": "common",
          "children": [
            {
              "path": "overlay",
              "children": [
                {
                  "path": "color",
                  "page": {
                    "githubLink": "components/common/overlay/color",
                    "docURI": "/libs/components/common/overlay/color/README.md",
                    "controls": [{
                      "controlType": "select",
                      "key": "color",
                      "label": "Color",
                      "value": "blue",
                      "optionsId": "OVERLAY_COLORS"
                    }, {
                      "controlType": "input",
                      "key": "allow-transition",
                      "label": "Allow Transition",
                      "type": "checkbox",
                      "value": "true"
                    }],
                    "examples": [
                      "color-overlay-example"
                    ]
                  }
                }
              ]
            }
          ]
        }, {
          "path": "layout",
          "children": [{
            "path": "page-header",
            "page": {
              "detailsURI": "details/page-header/README.md",
              "docURI": "/libs/product/components/layout/headers/page/README.md",
              "controls": [{
                "controlType": "header",
                "label": "Inputs"
              }, {
                "controlType": "input",
                "key": "fallbackSeed",
                "label": "Fallback Seed",
                "type": "number",
                "value": "1"
              }, {
                "controlType": "select",
                "key": "fallbackColor",
                "label": "Fallback Color",
                "value": "blue",
                "optionsId": "FALLBACK_COLORS"
              }, {
                "controlType": "select",
                "key": "overlayColor",
                "label": "Overlay Color",
                "value": "blue",
                "optionsId": "OVERLAY_COLORS"
              }, {
                "controlType": "select",
                "key": "imageUrl",
                "label": "Image Url",
                "value": "none",
                "options": [
                  {"key": "none", "value": "none"},
                  {"key": "image-1", "value": "/assets/media-examples/sample.png", "label": "Image 1"},
                  {"key": "image-2", "value": "/assets/media-examples/sample_2.png", "label": "Image 2"}
                ]
              }, {
                "controlType": "input",
                "key": "title",
                "label": "Title",
                "type": "text",
                "value": "Header Title"
              }, {
                "controlType": "divider"
              }, {
                "controlType": "header",
                "label": "Content slots"
              }, {
                "controlType": "input",
                "key": "topSlotContentHeight",
                "label": "Top Slot Content Height",
                "type": "number",
                "value": "0"
              }, {
                "controlType": "input",
                "key": "contentSlotContentHeight",
                "label": "Content Slot Content Height",
                "type": "number",
                "value": "0"
              }],
              "examples": [
                "page-header-example"
              ]
            }
          }]
        }, {
            "path": "media",
            "children": [
              {
                "path": "image",
                "children": [
                  {"path": "fallback"},
                  {"path": "image-component"}
                ]
              }
            ]
          }, {
            "path": "lesson-plan",
            "children": [
              {"path": "card"},
              {"path": "hero"},
              {"path": "grid"},
              {"path": "header"},
              {"path": "vocabulary"}
            ]
          }, {
            "path": "my-library",
            "children": [
              {"path": "lesson-plan-card"}
            ]
          }
        ]
      }]
    }, {
      "path": "core",
      "children": [{
        "path": "analytics",
        "page": {
          "detailsURI": "details/analytics/README.md",
          "docURI": "libs/core/analytics/README.md",
          "githubLink": "libs/core/analytics",
          "sections": [{
            "label": "Analytics NgModule",
            "isOpen": false,
            "docURI": "libs/ng/core/analytics/README.md",
            "githubLink": "libs/ng/core/analytics/analytics.module.ts"
          }]
        }
      }, {
        "path": "commands",
        "page": {
          "detailsURI": "details/commands/README.md",
          "docURI": "libs/core/commands/README.md",
          "githubLink": "core/commands",
          "sections": [{
            "label": "Command",
            "isOpen": false,
            "docURI": "libs/core/commands/command/README.md",
            "githubLink": "libs/core/commands/command/command.ts"
          }, {
            "label": "Group",
            "isOpen": false,
            "docURI": "libs/core/commands/group/README.md",
            "githubLink": "libs/core/commands/group/command-group.ts"
          }, {
            "label": "Processor",
            "isOpen": false,
            "docURI": "libs/core/commands/processor/README.md",
            "githubLink": "libs/core/commands/processor/command-processor.ts"
          }]
        }
      }, {
        "path": "http",
        "page": {
          "detailsURI": "details/http/README.md",
          "docURI": "libs/core/http/README.md",
          "githubLink": "libs/core/http",
          "sections": [{
            "label": "Http NgModule",
            "isOpen": false,
            "docURI": "libs/ng/core/http/README.md",
            "githubLink": "libs/ng/core/http/http.module.ts"
          }]
        }
      }]
    }
  ],
  "ga-analytics": {
    "trackerId": "G-EJZC3TV0QZ",
    "trackerName": "",
    "cookieDomain": ""
  },
  "analytics" : {
    "page": {
      "trackType": "page",
      "properties": {
        "page_title": "{%title%}",
        "page_path": "{%path%}",
        "page_location": "{%href%}"
      }
    },
    "github-link" : {
      "type": "github-link",
      "trackType": "event",
      "properties": {
        "event_category": "links",
        "event_label": "{%link%}"
      }
    },
    "child-section": {
      "type": "child-section.expand",
      "trackType": "event",
      "properties": {
        "event_category": "page",
        "event_label": "{%label%}",
        "value": "{%value%}"
      }
    },
    "href-link": {
      "type": "href-link",
      "trackType": "event",
      "properties": {
        "event_category": "links",
        "event_label": "{%link%}"
      }
    },
    "menu": {
      "parent-item": {
        "type": "parent-item-click",
        "trackType": "event",
        "properties": {
          "event_category": "menu",
          "event_label": "{%label%}",
          "value": "{%value%}"
        }
      },
      "item": {
        "type": "item-click",
        "trackType": "event",
        "properties": {
          "event_category": "menu",
          "event_label": "{%label%}"
        }
      }
    }
  },
  "github-config": {
      "root": "https://github.com/jamesbrobb/portfolio/",
      "app": "tree/main/src/app/"
  }
}
