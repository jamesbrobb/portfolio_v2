
{
  "routes": [{
      "path": "/",
      "redirectTo": "about"
    },
    {
      "path": "about",
      "githubLink": "/",
      "docURI": "details/home/README.md"
    }, {
      "path": "components",
      "children": [{
        "path": "common",
        "children": [
          {
            "path": "overlay",
            "children": [
              {
                "path": "color",
                "githubLink": "components/common/overlay/color",
                "docURI": "/libs/components/common/overlay/color/README.md",
                "controls": [{
                  "controlType": "select",
                  "key": "color",
                  "label": "Color",
                  "value": "blue",
                  "optionsId": "OVERLAY_COLORS"
                }, {
                  "controlType": "input",
                  "key": "allow-transition",
                  "label": "Allow Transition",
                  "type": "checkbox",
                  "value": "true"
                }],
                "examples": [
                  "color-overlay-example"
                ]
              }
            ]
          }
        ]
      }, {
        "path": "layout",
        "children": [
          {
            "path": "grid",
            "githubLink": "components/layout/grid",
            "docURI": "/libs/components/layout/grid/README.md",
            "examples": [
              "grid-layout-example"
            ]
          },
          {
            "path": "flex-grid"
          },
          {
            "path": "responsive-container",
            "githubLink": "components/responsive",
            "docURI": "/libs/components/responsive/container/README.md",
            "examples": [
              "responsive-container-example"
            ]
          }, {
            "path": "page-header",
            "detailsURI": "details/page-header/README.md",
            "docURI": "/libs/product/components/layout/headers/page/README.md",
            "controls": [{
              "controlType": "header",
              "label": "Inputs"
            }, {
              "controlType": "input",
              "key": "fallbackSeed",
              "label": "Fallback Seed",
              "type": "number",
              "value": "1"
            }, {
              "controlType": "select",
              "key": "fallbackColor",
              "label": "Fallback Color",
              "value": "blue",
              "optionsId": "FALLBACK_COLORS"
            }, {
              "controlType": "select",
              "key": "overlayColor",
              "label": "Overlay Color",
              "value": "blue",
              "optionsId": "OVERLAY_COLORS"
            }, {
              "controlType": "select",
              "key": "imageUrl",
              "label": "Image Url",
              "value": "none",
              "options": [
                {"key": "none", "value": "None"},
                {"key": "/assets/media-examples/sample.png", "value": "Image 1"},
                {"key": "/assets/media-examples/sample_2.png", "value": "Image 2"}
              ]
            }, {
              "controlType": "input",
              "key": "title",
              "label": "Title",
              "type": "text",
              "value": "Header Title"
            }, {
              "controlType": "divider"
            }, {
              "controlType": "header",
              "label": "Content slots"
            }, {
              "controlType": "input",
              "key": "topSlotContentHeight",
              "label": "Top Slot Content Height",
              "type": "number",
              "value": "0"
            }, {
              "controlType": "input",
              "key": "contentSlotContentHeight",
              "label": "Content Slot Content Height",
              "type": "number",
              "value": "0"
            }],
            "examples": [
              "page-header-example"
            ]
          }
        ]
      }, {
        "path": "media",
        "children": [
          {
            "path": "image",
            "children": [
              {"path": "fallback"},
              {"path": "image-component"}
            ]
          }
        ]
      }, {
        "path": "product",
        "children": [
          {
            "path": "lesson-plan",
            "children": [
              {"path": "card"},
              {"path": "hero"},
              {"path": "grid"},
              {"path": "header"},
              {"path": "vocabulary"}
            ]
          }, {
            "path": "my-library",
            "children": [
              {"path": "lesson-plan-card"}
            ]
          }
        ]
      }]
    }, {
      "path": "core",
      "children": [
        {"path": "analytics"},
        {"path": "commands"},
        {"path": "http"}
      ]
    }
  ],
  "ga-analytics": {
    "trackerId": "G-EJZC3TV0QZ",
    "trackerName": "",
    "cookieDomain": ""
  },
  "analytics" : {
    "page": {
      "trackType": "page",
      "properties": {
        "page_title": "{%title%}",
        "page_path": "{%path%}",
        "page_location": "{%href%}"
      }
    },
    "github-link" : {
      "type": "github-link",
      "trackType": "event",
      "properties": {
        "event_category": "links",
        "event_label": "{%link%}"
      }
    },
    "child-section": {
      "type": "child-section.expand",
      "trackType": "event",
      "properties": {
        "event_category": "page",
        "event_label": "{%label%}",
        "value": "{%value%}"
      }
    },
    "href-link": {
      "type": "href-link",
      "trackType": "event",
      "properties": {
        "event_category": "links",
        "event_label": "{%link%}"
      }
    },
    "menu": {
      "parent-item": {
        "type": "parent-item-click",
        "trackType": "event",
        "properties": {
          "event_category": "menu",
          "event_label": "{%label%}",
          "value": "{%value%}"
        }
      },
      "item": {
        "type": "item-click",
        "trackType": "event",
        "properties": {
          "event_category": "menu",
          "event_label": "{%label%}"
        }
      }
    }
  },
  "github-config": {
      "root": "https://github.com/jamesbrobb/portfolio/",
      "app": "tree/main/src/app/"
  }
}
